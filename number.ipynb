   "source": [
    "from itertools import product\n",
    "\n",
    "K, M = map(int,input().split())\n",
    "\n",
    "N = (list(map(int, input().split()))[1:] for _ in range(K))\n",
    "\n",
    "max_lst = []\n",
    "for item in product(*N):\n",
    "    S = 0\n",
    "    for val in item:\n",
    "        S += val**2\n",
    "    S_max = S % M\n",
    "    max_lst.append(S_max)\n",
    "    \n",
    "print(max(max_lst))"
   ]
  }
